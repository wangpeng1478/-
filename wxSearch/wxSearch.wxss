.wxSearch{
  position: absolute;
  top: 86rpx;
  left: 0;
  width: 100%;
  height: 100%;
  border-top: 1px #eee solid;
  background-color:#fafafa;
}
.wxSearchInner{
  background-color: #fafafa;
}
.wxSearch-section{
  background-color: #fff;
  padding: 10rpx 20rpx;
  border: 1px #eee solid;
}
.wxSearch-pancel{
  display: flex;
  flex-direction: row;
}
.wxSearch-input{
  flex: 8; 
  background-color: #eee;
  padding: 10rpx 20rpx;
}
.wxSearch-button{
  flex: 1;
  border: none !important;
  color: cornflowerblue !important;
}
.wxSearchTitle{
  display: block;
  padding: 10rpx;
  font-size: 25rpx;
  text-align: left;
  color: #444444;
  border-left: 5rpx solid #00b86b;
  font-weight: bold;
}
.wxSearchMindKeyItem{
  padding: 15rpx 0 15rpx 35rpx;
  border-bottom: 1px solid #eee;
  display: flex;
  font-size: 25rpx;
  color:#939393;
}
.wxSearchKeyList{
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
}

.wxSearchKeyItem{
  font-size: 24rpx;
  text-align: center;
  border: 1px solid #eee;
  margin: 10rpx;
  padding: 10rpx 20rpx;
  border-radius: 10rpx;
  color: #939393;
}
.wxSearchHistoryItem{
  padding: 20rpx 10rpx;
  border-bottom: 1px solid #eee;
  display: flex;
  align-items: center;
}
.wxSearchHistoryItemText{
  flex: 8;
  font-size: 25rpx;
  padding-left: 15rpx;
  color: #666;
}
.wxSearchHistoryItemDel{
  flex: 1;text-align:center;
  
}
.wxSearchHistoryEmpty{
  text-align: center;
  height: 100rpx;
  line-height: 100rpx;
  font-size: 24rpx;
  color:#666;
}
.wxSearchDeleteAllBtn{
  font-size: 24rpx;
  width: 200rpx;
  margin: 20rpx auto;
  text-align: center;
  border:1px solid #00b86b;
  color: #00b86b;
  padding: 10rpx 15rpx;
}



