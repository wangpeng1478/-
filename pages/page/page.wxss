/* 直接设置swiper属性 */

swiper {
    height: 500rpx;
    z-index: 2
}

swiper-item image {
    width: 100%;
    height: 100%;
}

.detail {
    display: flex;
    flex-direction: column;
    margin-top: 15rpx;
    margin-bottom: 0rpx;
}

.swiper-box .wx-swiper-dots.wx-swiper-dots-horizontal {
    margin-bottom: 2rpx;
}

.swiper-box .wx-swiper-dot {
    width: 20rpx;
    display: inline-flex;
    height: 5rpx;
    margin-left: 5rpx;
    justify-content: space-between;
    margin-bottom: 20rpx;
}

.swiper-box .wx-swiper-dot::before {
    content: '';
    flex-grow: 1;
    background: rgba(255, 255, 255, 0.5);
    border-radius: 8rpx;
}

.Cphoen {
