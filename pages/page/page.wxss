.map {
    width: 100%;
    height: 400rpx;
    margin: 10rpx 0;
    background-size: 100% auto;
    position: relative;
}
.map image{
    width: 100%;
    height: 100%;
}
.map .mapBG{
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    margin: auto;
    width: 50rpx;
    height: 70rpx;
   background: transparent url("data:image/png;base64,") top center no-repeat;
   background-size: 40rpx auto;
    -webkit-animation: tiaotiao 1s infinite ease-out;
    animation: tiaotiao 1s infinite ease-out;
}
.map .mapT{
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    margin: auto;
    max-width: 200rpx;
    height: 50rpx;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
    line-height: 50rpx;
    padding: 0 5rpx;
    background: rgba(0,0,0,.7);
    text-align: center;
    font-size: 23rpx;
    color: #fff;
    border-radius: 25rpx;
    transform: translateY(30rpx);
    -webkit-transform: translateY(30rpx)
}

@-webkit-keyframes tiaotiao {
    0%{
        -webkit-transform: translateY(0)
    }
    50% {
        -webkit-transform: translateY(-30rpx)
    }
    100% {
        -webkit-transform: translateY(0)
    }
}
@keyframes tiaotiao {
    0% {
        transform: translateY(0)
    }
    50% {
        transform: translateY(-30rpx)
    }
    100% {
        transform: translateY(0)
    }
}
