    margin: 10rpx 0;
}

.swiper-box .wx-swiper-dot-active::before {
    background: #00b86b;
}

.containerr {
    background: transparent;
    width: 95%;
    margin: -20rpx auto 0 auto;
    z-index: 2;
}

.containerr .page_box {
    background: #fff;
    padding: 5rpx 20rpx;
    margin-bottom: 15rpx;
    box-shadow: 0 0 20px 2px rgba(0, 0, 0, 0.15);
}

.containerr .page_box:first-child {
    border-top-left-radius: 10rpx;
    border-top-right-radius: 10rpx;
}

.page_box .page_box_h2 {
    font-size: 34rpx;
    padding: 10rpx 0;
    font-weight: 700;
    color: #76695b;
}

.page_box_ul .l {
    float: left;
}

.page_box_ul .l {
    width: 80%;
    font-size: 25rpx;
    color: #76695b;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}

.page_box_ul .l .d::before, .page_box_ul .l .t::before {
    content: "";
    display: inline-block;
    vertical-align: -2rpx;
    width: 25rpx;
    height: 25rpx;
    padding-right: 10rpx;
}

.page_box_ul .l .d::before {
    background: transparent url("data:image/png;base64,") center center no-repeat;
    background-size: 25rpx auto;
}

.page_box_ul .l .t::before {
    background: transparent url("data:image/png;base64,") center center no-repeat;
    background-size: 25rpx auto;
}

.page_box_ul .r {
