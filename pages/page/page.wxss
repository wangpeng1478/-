.extend .clichide {
    text-indent: 0;
    width: 100%;
    position: absolute;
    bottom: 0;
    left: 0;
    text-align: center;
    background: -webkit-linear-gradient(top, rgba(255, 255, 255, 0.5), #fff 100%);
    color: #00b86b;
    font-size: 28rpx;
    padding-top: 20rpx;
}



.clichide .textloog {
    width: 50rpx;
    height: 50rpx;
    margin: 0 auto;
    background: transparent;
    -webkit-animation: topbo 1s infinite linear;
    animation: topbo 1s infinite linear;
}

@-webkit-keyframes topbo {
    from {
        transform: translateY(0);
        opacity: 1;
    }

    to {
        transform: translateY(20rpx);
        opacity: 0;
    }
}

@keyframes topbo {
    from {
        transform: translateY(0);
        opacity: 1;
    }

    to {
        transform: translateY(15rpx);
        opacity: 0;
    }
}

