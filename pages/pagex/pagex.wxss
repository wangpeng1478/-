@import "../../pages/page/page.wxss";

.page_box_h3 {
    font-size: 30rpx;
    padding: 10rpx 0;
    font-weight: 600;
    color: #76695b;
}

.pagexbox .li {
    font-size: 24rpx;
    color: #665c52;
    margin-bottom: 8rpx;
    float: left;
    width: 100%;
}

.pagexbox .lif {
    width: 48%;
}

.page_for .t3 {
    font-size: 22rpx;
}

.page_for .t3 text {
    font-size: 28rpx;
    color: #00b86b;
    font-weight: 500;
    margin: 2rpx;
}

.page_for .t4 {
    font-size: 24rpx;
}

.mingpian {
    position: relative;
    color: #76695b;
    padding: 8rpx 0;
}

.mingpian image {
    width: 120rpx;
    height: 120rpx;
    border: 1px solid #eee;
    border-radius: 60rpx;
    float: left;
}

.mingpian .t {
    padding: 5rpx;
    float: left;
    text-align: left;
    font-size: 23rpx;
    white-space: nowrap;
    margin-left: 10rpx;
}

.mingpian .t text {
    font-size: 35rpx;
    padding-right: 15rpx;
    font-weight: 500;
}

.mingpian .q1 {
    margin-top: 13rpx;
}

.mingpian button {
    position: absolute;
    top: 17rpx;
    right: 10rpx;
    font-size: 23rpx;
    background: #f2f2f2 url("data:image/png;base64,") 10rpx center no-repeat;
    background-size: 30rpx auto;
    padding-left: 45rpx;
    padding-right: 10rpx;
}
