.swiper-tab {
    width: 100%;
    text-align: center;
    height: 80rpx;
    line-height: 80rpx;
    display: flex;
    background: #00b86b;
    position: fixed;
    top: 0;
    left: 0;
    z-index: 10;
}

.pof {
    height: 80rpx;
}

.swiper-tab-list {
    font-size: 25rpx;
    display: inline-block;
    width: 33.333%;
    color: #fff;
}

.on {
    color: #fff;
    font-weight: bold;
    border-bottom: 5rpx solid #007a48;
}

.swiper-box {
    display: block;
    min-height: 100%;
    width: 100%;
    overflow: hidden;
    z-index: 1;
}

.foot {
    background: #fff;
}

.foot .p, .foot .Cphoen {
    display: none;
}

.se1 .se1bg {
    display: block;
    width: 700rpx;
    height: 250rpx;
    margin: 30rpx auto;
    border-radius: 5rpx;
    box-shadow: 0 0 40rpx 10rpx rgba(0, 0, 0, 0.2);
}

.se1 .p {
    width: 700rpx;
    margin: 10rpx auto;
    text-indent: 2em;
    color: #666;
    font-size: 28rpx;
}

.se2 .se1bg {
    display: block;
    width: 700rpx;
    height: 250rpx;
    margin: 30rpx auto;
    border-radius: 5rpx;
    box-shadow: 0 0 40rpx 10rpx rgba(0, 0, 0, 0.2);
}

.se2 .p1 {
    width: 700rpx;
    margin: 10rpx auto;
    text-indent: 2em;
    color: #666;
    font-size: 28rpx;
}

.se2 .semap {
    width: 700rpx;
    margin: 75rpx auto 0 auto;
    height: 500rpx;
    position: relative;
}

.se2 .semap image {
    width: 100%;
    height: 100%;
    box-shadow: 0 0 40rpx 10rpx rgba(0, 0, 0, 0.2);
}

.se2 .semap .mapBG {
    position: absolute;
    top: 150rpx;
    left: 0;
    right: 0;
    bottom: 0;
    z-index: 10;
    margin: auto;
    width: 50rpx;
    height: 70rpx;
   background: transparent url("data:image/png;base64,") top center no-repeat;
   background-size: 40rpx auto;
    -webkit-animation: tiaotiao 1s infinite ease-out;
    animation: tiaotiao 1s infinite ease-out;
}

.se2 .semap .mapT {
    position: absolute;
    top: 263rpx;
    left: 0;
    right: 0;
    bottom: 0;
     z-index: 10;
    margin: auto;
    max-width: 200rpx;
    height: 50rpx;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
    line-height: 50rpx;
    padding: 0 5rpx;
    background: rgba(0,0,0,.7);
    text-align: center;
    font-size: 23rpx;
    color: #fff;
    border-radius: 25rpx;
}

@-webkit-keyframes tiaotiao {
    0% {
        -webkit-transform: translateY(0);
    }

    50% {
        -webkit-transform: translateY(-30rpx);
    }

    100% {
        -webkit-transform: translateY(0);
    }
}

@keyframes tiaotiao {
    0% {
        transform: translateY(0);
    }

    50% {
        transform: translateY(-30rpx);
    }

    100% {
        transform: translateY(0);
    }
}

.se2 .semap .but{
    width: 100%;
    display: flex;
    position: absolute;
    top: 15rpx;
    left: 0;
     box-shadow: 0 0 40rpx 10rpx rgba(0, 0, 0, 0.2);
}
.se2 .semap .but  .b1{
   display: inline-block;
   background: rgba(0,0,0,.7); 
   color: #fff;
   width: 50%;
   padding: 10rpx 20rpx;
   text-align: center;
   font-size: 25rpx;
   -webkit-transition:all 0.25s ease-out;
   transition:all 0.25s ease-out;
}
.se2 .semap .but  .b1:first-child{
    border-right: 1px solid #fff;
}
.se2 .semap .but .abon{
    background: #00b86b;
}
